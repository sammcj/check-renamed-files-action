{
  "name": "check-renamed-files",
  "version": "1.2.0",
  "description": "Github Action to check if any files have been renamed between two git refs in a given path",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "check-prettier": "npm run prettier --fix",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 \"./**/*.{js,ts}\"",
    "format-prettier": "npm run prettier --write",
    "prettier": "prettier --ignore-path .gitignore \"./**/*.{js,json,md,ts,yml}\"",
    "build": "npx @vercel/ncc build -m index.js --minify --v8-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammcj/flyway-changes-action.git"
  },
  "keywords": [],
  "author": "Sam McLeod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sammcj/flyway-changes-action/issues"
  },
  "homepage": "https://github.com/sammcj/flyway-changes-action#readme",
  "devDependencies": {
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.3.1",
    "lint-staged": "13.0.4",
    "prettier": "2.8.0"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "chalk": "5.1.2",
    "simple-git": "3.15.1",
    "@vercel/ncc": "0.34.0"
  }
}
